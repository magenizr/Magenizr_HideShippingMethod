{
  "name": "magenizr/magento2-hideshippingmethod",
  "description": "Hide Shipping Method allows admin users to hide one or multiple shipping methods on the cart and checkout page. This can be limited to specific weekdays, by start and end-date or customer sessions.",
  "type": "magento2-module",
  "version": "1.0.0",
  "license": [
    "OSL-3.0"
  ],
  "authors": [
    {
      "name": "Magenizr",
      "email": "modules@magenizr.com",
      "homepage": "https://www.magenizr.com"
    }
  ],
  "require": {
    "magento/framework": "^100|^101",
    "magento/module-backend": "^100|^101",
    "phpsysinfo/phpsysinfo": "^3"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "keywords": [
    "Magento 2",
    "Admin",
    "Shipping",
    "Method",
    "Hide"
  ],
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Magenizr\\HideShippingMethod\\": ""
    }
  }
}